/div>\n            <div class=\"field\">\n              <div class=\"ui left icon input\">\n                <i class=\"lock icon\"></i>\n                <input\n                  type=\"password\"\n                  name=\"password\"\n                  placeholder=\"Password\"\n                  v-model=\"password\"\n                />\n              </div>\n            </div>\n            <button class=\"ui fluid large red submit button\" v-on:click= \"submit;next\">\n              Next\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport firebase from \"firebase\";\nexport default {\n  name: \"signUp\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      success: false\n    };\n  },\n  methods: {\n    submit: function() {\n      firebase\n        .auth()\n        .createUserWithEmailAndPassword(this.email, this.password)\n        .then(\n          function(user) {\n            console.log(\"SUCCESS\");\n            this.success= true;\n            alert(user + \"Your account has been created!\");\n          },\n          function(err) {\n            alert(\"oops\" + err.message);\n          }\n        );\n    },\n    next:function(){\n      if(this.success===true){\n        this.$router.push('/merchant/form');\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\nbody > .grid {\n  height: 300%;\n}\n.image {\n  margin-top: -100px;\n}\n.column {\n  max-width: 600px;\n}\n</style>\n"]}]}