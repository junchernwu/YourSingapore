rgba(82, 82, 100, 0.554); \r\n    \r\n}\r\n#value{\r\n  font-weight:bold;\r\n  text-transform: uppercase;\r\n  font-size:30px;\r\n  line-height: 10px;\r\n}\r\n.time ul{\r\n  list-style-type: none;\r\n  display:flex;  \r\n  position:relative;\r\n  margin-right:330px; \r\n  \r\n}\r\n.time li{\r\n  padding-left:5px;\r\n  \r\n}\r\n.leftt{\r\n  float:left;\r\n  width:50%;\r\n}\r\n.rightt{\r\n  float:left;\r\n  width:50%;\r\n}\r\n#box2{\r\n  height:230px;\r\n}\r\n</style>  "]}]}